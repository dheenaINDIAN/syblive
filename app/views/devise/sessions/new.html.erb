
<div class="logo"><a href="#">
<img src="/assets/ZatFish_Logo.png" width="540" height="235"  align="middle"/></a></div>
<div class="Sign-In">
<h4>Sign In</h4>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<p class="float-LT">
<%= f.label :email %>
<%= f.text_field :email %>
</p>

<p class="float-LT">
<%= f.label :password %>
<%= f.password_field :password %>
</p>




<%= f.submit "Sign up" %>


<% end %>

<%= render :partial => "devise/shared/links" %>